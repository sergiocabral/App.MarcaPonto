<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:id="@+id/widget39"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	android:orientation="vertical" xmlns:android="http://schemas.android.com/apk/res/android"
	android:gravity="center">

	<LinearLayout android:id="@+id/LinearLayout01"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:layout_marginBottom="5dp" android:orientation="vertical">

		<LinearLayout android:id="@+id/LinearLayout03"
			android:layout_width="fill_parent" android:layout_height="wrap_content"
			android:gravity="center|right">

			<TextView android:text="@string/JanelaConfiguracaoDasHoras_lblHoraEntradaMenor"
				android:id="@+id/lblHoraEntradaMenor" android:layout_width="wrap_content"
				android:layout_height="wrap_content">
			</TextView>

			<Spinner android:id="@+id/txtHoraEntradaMenor"
				android:layout_width="wrap_content" android:layout_height="wrap_content"
				androidrompt="@string/JanelaConfiguracaoDasHoras_lblHoraEntradaMenor">
			</Spinner>

		</LinearLayout>

		<LinearLayout android:id="@+id/LinearLayout04"
			android:layout_width="fill_parent" android:layout_height="wrap_content"
			android:gravity="center|right">

			<TextView android:text="@string/JanelaConfiguracaoDasHoras_lblHoraEntradaMaior"
				android:id="@+id/lblHoraEntradaMaior" android:layout_width="wrap_content"
				android:layout_height="wrap_content">
			</TextView>

			<Spinner android:id="@+id/txtHoraEntradaMaior"
				android:layout_width="wrap_content" android:layout_height="wrap_content"
				androidrompt="@string/JanelaConfiguracaoDasHoras_lblHoraEntradaMaior">
			</Spinner>

		</LinearLayout>

		<LinearLayout android:id="@+id/LinearLayout05"
			android:layout_width="fill_parent" android:layout_height="wrap_content"
			android:gravity="center|right">

			<TextView android:text="@string/JanelaConfiguracaoDasHoras_lblHoraAlmoco"
				android:id="@+id/lblHoraAlmoco" android:layout_width="wrap_content"
				android:layout_height="wrap_content">
			</TextView>

			<Spinner android:id="@+id/txtHoraAlmoco"
				android:layout_width="wrap_content" android:layout_height="wrap_content"
				androidrompt="@string/JanelaConfiguracaoDasHoras_lblHoraAlmoco">
			</Spinner>

		</LinearLayout>

		<LinearLayout android:id="@+id/LinearLayout06"
			android:layout_width="fill_parent" android:layout_height="wrap_content"
			android:gravity="center|right">

			<TextView android:text="@string/JanelaConfiguracaoDasHoras_lblHoraJornada"
				android:id="@+id/lblHoraJornada" android:layout_width="wrap_content"
				android:layout_height="wrap_content">
			</TextView>

			<Spinner android:id="@+id/txtHoraJornada"
				android:layout_width="wrap_content" android:layout_height="wrap_content"
				androidrompt="@string/JanelaConfiguracaoDasHoras_lblHoraJornada">
			</Spinner>

		</LinearLayout>

	</LinearLayout>

	<LinearLayout android:layout_height="wrap_content"
		android:gravity="center" android:layout_width="fill_parent"
		android:id="@+id/LinearLayout02" android:layout_marginTop="10dp">

		<Button android:text="@string/Geral_btnSalvar" android:id="@+id/btnSalvar"
			android:layout_width="90dp" android:layout_height="wrap_content">
		</Button>

		<Button android:text="@string/Geral_btnCancelar" android:id="@+id/btnCancelar"
			android:layout_width="90dp" android:layout_height="wrap_content">
		</Button>

	</LinearLayout>

</LinearLayout>
